                <script>
                  $(".qtyBtn").on("click", function() {
                    var oldValue = jQuery("#Quantity").val(),
                        newVal = 1;                     

                    if ($(this).is(".plus")) {
                      newVal = parseInt(oldValue) + 1;
                    } else if (oldValue > 1) {
                      newVal = parseInt(oldValue) - 1;
                    }
                    $("#Quantity").val(newVal);
                  });
                </script>
